    public static void printStars(int amount)

    int printedStars = 0;

while(printedStars<amount)

    {
        System.out.print("*");
        printedStars = printedStars + 1;
    }
System.out.println("");
}

    public static void printTriangle(int size) {
        for (int y = 0; y < size; y++) {
            printStars(y + 1);
        }
    })

